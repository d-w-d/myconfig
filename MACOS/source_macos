#!/usr/bin/env false

##############################################################
### Inherit $MYCONFIG_ROOT_DIR from parent script
##############################################################
MYCONFIG_ROOT_DIR=${MYCONFIG_ROOT_DIR:-"$HOME/.myconfig"}

# Source 'em:
source $MYCONFIG_ROOT_DIR/MACOS/macos_aliases
source $MYCONFIG_ROOT_DIR/MACOS/macos_env
source $MYCONFIG_ROOT_DIR/MACOS/macos_functions
